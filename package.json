{
  "name": "css-modules-kit-monorepo",
  "type": "module",
  "repository": "https://github.com/mizdra/css-modules-kit.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/codegen",
    "packages/ts-plugin",
    "packages/vscode",
    "packages/stylelint-plugin",
    "packages/eslint-plugin"
  ],
  "scripts": {
    "version": "changeset version && npm i",
    "build": "tsc -b tsconfig.build.json",
    "lint": "run-s -c lint:*",
    "lint:tsc": "tsc -b",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --cache --check .",
    "test": "vitest --run --project unit",
    "e2e": "run-s -c e2e:build e2e:vitest e2e:vscode",
    "e2e:build": "npm run build",
    "e2e:vitest": "vitest --run --project e2e",
    "e2e:vscode": "vscode-test"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "devDependencies": {
    "@eslint/css": "^0.4.0",
    "@mizdra/eslint-config-mizdra": "^6.1.0",
    "@mizdra/inline-fixture-files": "^2.1.1",
    "@mizdra/prettier-config-mizdra": "^2.0.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.10",
    "@types/postcss-safe-parser": "^5.0.4",
    "@types/react": "^19.0.10",
    "@types/vscode": "^1.98.0",
    "@typescript/server-harness": "^0.3.5",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "dedent": "^1.5.3",
    "eslint": "^9.22.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
