{
  "name": "@css-modules-kit/codegen",
  "description": "A tool for generating `*.d.ts` files for `*.module.css`.",
  "version": "0.8.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mizdra/css-modules-kit.git",
    "directory": "packages/codegen"
  },
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -b tsconfig.build.json"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "cmk": "bin/cmk.js"
  },
  "keywords": [
    "css-modules",
    "typescript"
  ],
  "files": [
    "bin",
    "src",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/test",
    "dist"
  ],
  "dependencies": {
    "@css-modules-kit/core": "^0.8.0",
    "chokidar": "^4.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  }
}
