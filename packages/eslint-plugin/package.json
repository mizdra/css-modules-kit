{
  "name": "@css-modules-kit/eslint-plugin",
  "description": "A eslint plugin for CSS Modules",
  "version": "0.2.1",
  "type": "commonjs",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mizdra/css-modules-kit.git",
    "directory": "packages/eslint-plugin"
  },
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "css-modules",
    "eslint",
    "eslint-plugin"
  ],
  "files": [
    "bin",
    "src",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/test",
    "dist"
  ],
  "dependencies": {
    "@css-modules-kit/core": "^0.4.0",
    "postcss-safe-parser": "^7.0.1"
  },
  "peerDependencies": {
    "@eslint/css": ">=0.4.0",
    "eslint": ">=9.0.0"
  }
}
